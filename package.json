{
  "name": "node-hello-world",
  "version": "0.0.1",
  "description": "Node / Express / ES6 Demo app",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "node dist",
    "prestart": "npm run -s build",
    "lint": "eslint src",
    "test": "mocha tests/index.js"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "repository": "oscmejia/node-hello-world",
  "author": "oscmejia <oscmejia@icloud.com>",
  "license": "TBD",
  "dependencies": {
    "body-parser": "^1.13.3",
    "compression": "^1.5.2",
    "cors": "^2.7.1",
    "express": "^4.13.3",
    "lodash": "^4.16.1",
    "mongodb": "^2.2.10",
    "mongoose": "^4.6.1"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.14.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "eslint": "^3.1.1",
    "mocha": "^3.0.2",
    "mongo-clean": "^1.2.2",
    "nodemon": "^1.9.2",
    "request": "^2.75.0"
  }
}
